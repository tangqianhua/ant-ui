{
  "name": "t-ant-ui",
  "version": "0.1.0",
  "private": false,
  "main": "dist/index.js",
  "module": "dist/index.js",
  "typings": "dist.index.d.ts",
  "keywords": ["UI", "React"],
  "files": ["dist"],
  "repository": {
    "type": "git",
    "url": "https://github.com/tangqianhua/ant-ui"
  },
  "homepage": "https://github.com/tangqianhua/ant-ui",
  "dependencies": {
    "axios": "^0.21.1",
